{
  "manifest_version": 2,

  "name": "Page Editor",
  "description": "Edit your current page on the fly. Export it as an Image or Print it directly.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Page Editor"
  },
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": ["main.js"],
    "persistent": false
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
